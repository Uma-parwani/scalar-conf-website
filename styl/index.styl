@import "normalize.min.css"
@import "foundation.min.css"
@import "nib"

html
  min-height: 100%;

body
  background: url('data:image/png;base64,') center top repeat-y, url('images/top.png') center top no-repeat, url('images/bottom.png') center bottom no-repeat, linear-gradient(20px top, #32c1d0, #114476);
  font-family: 'Exo 2', 'Exo 2.0', sans-serif;
  text-shadow: 0 0 1px rgba(17, 68, 118, 0.3);

// Start fixing Foundation styles
h1
h2
h3
h4
h5
h6
  font-family: 'Exo 2', 'Exo 2.0', sans-serif;
  -webkit-text-stroke: 0.6px rgba(255,255,255,0.8);

h1
  font-size: 2.25em;
  font-weight: 200;
  text-align: center;
  text-transform: uppercase;

p
  font-size: 0.875rem;
  line-height: 1.48;

button, 
.button
  border: 2px solid white;
  color: #2184a4;
  font-family: inherit;
  font-size: 0.875rem;
  padding: .375rem 1rem;
  &:hover
    background: transparent;
    color: white;

  &.postfix
    border: 2px solid white;

fieldset
  border: none;

.top-bar
  background: transparent;
  height: 59px;

  & .name
    height: 59px;

    & h1
      
      & a
        padding: 3px 0 0 1em;
        text-align: left;

        & img
          max-height: 54px;

  &.expanded
    & .title-area
      background: #2896b2;

.top-bar-section 
  & ul
    background: transparent;

    & li
      &:hover>a
        background: #1e638b;
      &>a
        background: #2896b2;
        font-family: 'Exo 2', 'Exo 2.0', sans-serif;
        // font-family: sans-serif;
        padding-left: 1em;
      &.active>a
        background: #1f7397;
        &:hover
          background: #1e638b;

.orbit-timer
  opacity: 0;

.orbit-container .orbit-prev:hover, .orbit-container .orbit-next:hover
  background-color: rgba(255, 255, 255, 0.3);
// End fixing Foundation styles


// Mailchimp form
#mce-EMAIL
  background: transparent;
  border: 2px solid white;
  box-shadow: none;
  color: white;
  &:focus
    box-shadow: inset 0 0 5px #2ca8bf;

#mc-embedded-subscribe
  &:focus
    background: white;
    color: #2184a4;

#note
  color: #fe232b;
  margin-bottom: 1em;
  text-align: center;

 #mce-success-response
  text-align: center;
  font-size: 0.84em;
  line-height: 1.3;

#mce-error-response
  text-align: center;
  font-size: 0.84em;
  line-height: 1.3;
  color: #fc4349;

#mc-embedded-subscribe
  padding: 0.5rem;
  width: 100%;
  &:focus
    outline: none;
// End Mailchimp


// Start general styles

a:hover img,
a:focus img
  opacity: 0.85;

.name a:hover img,
.name a:focus img
  opacity: 1;  

.centered-wrapper
  text-align: center;

.highlight
  color: #fc4349;
  -webkit-text-stroke: .6px rgba(252,67,73,0.6);

section.row
  background: url('data:image/png;base64,') center top no-repeat;
  padding: 8.125em 1em 0;
  position: relative;
  &:nth-child(odd)
    background-image: url('data:image/png;base64,');

  &.header
    background-image: none;
    padding: 0;
    margin-bottom: 1em;

    & li.tweet-btn
      padding: 0 15px;
      & a
        padding: 0;
      & iframe
        margin-top: 0.75rem; 

  &.hero
    background-image: none;
    padding-top: 3.125em;

    & h1
      color: #114476;
      font-size: 1.55em;
      font-weight: 600;
      -webkit-text-stroke-color: rgba(17, 68, 118, 0.8);
      text-shadow: none;

    & h2
      font-size: 1.25em;
      font-weight: 200;
      text-transform: uppercase;
      text-align: center;

    & .conference-data
      font-size: 1.25em;
      font-weight: 200;
      margin-top: 1em;
      -webkit-text-stroke: 0.6px rgba(255,255,255,0.8);

      & time,
      span
        display: block;
        margin-bottom: 0.5em;
        vertical-align: bottom;

      & time
        margin-right: 1em;

  &.sponsors
    background-image: none;
    padding-top: 1em;

    & .sponsors-slide
      text-align: center;

      & a
        display: inline-block;
        margin: 0 2em 2em;

  &.main-organizers
    text-align: center;

    & a
      display: inline-block;
      margin: 0 2em 2em;

      & small
        display: block;

    & h2
      color: #002447;
      font-size: 1.25em;
      font-weight: 200;
      text-transform: uppercase;
      text-align: center;
      -webkit-text-stroke: .6px rgba(0,36,71,0.8);

.to-top
  margin: .7em 5px 0 0;
  opacity: 0.4;
  position: absolute;
  right: 0;
  text-align: center;
  text-transform: uppercase;
  transition: opacity 500ms;
  z-index: 99;

  & img
    margin-bottom: 4px;
    backface-visibility: hidden;

  &:hover
    opacity: 1;
    color: white;

.section-title
  margin-bottom: 2.875em;
  text-align: center;

  & small
    font-size: .8125em;
    text-transform: uppercase;
// End general styles


// Start news section on index page
#news a.read-more
  margin-left: .75em;

.news-post
  padding: 0 .75em;
  margin-bottom: 1em;

  & .post-header
    margin-bottom: 1em;

  & h1
    font-size: 1.125rem;
    font-weight: 700;
    text-align: left;
    text-transform: none;
    -webkit-text-stroke: 0 transparent;

  & time
    color: #114476;
    font-size: .875rem;
    font-weight: 400;

.twitter-widget
  padding: 0 0.5em;
// End news section on index page


// Start agenda section
.agenda-table
  border-bottom: 1px dotted white;
  margin-bottom: 1rem;
  
  & .row
    border-top: 1px dotted white;
    // border-left: 1px dotted white;
    border-right: 1px dotted white;
    &:hover
      background: rgba(17, 68, 118, .3);
      transition: background 400ms;

    &.utility
      text-transform: uppercase;
      color: #002447;

      & .description
        text-align: center;

    & .time
      border-bottom: 1px dotted white;
      padding: 1em;
      text-transform: uppercase;
      text-align: center;

    & .description
      padding: 1em;

      & .talk
        margin-bottom: 1.32rem;

      & h1
        font-size: 1em;
        text-align: left;
        margin-top: 0;
        margin-bottom: 0;

      & h2
        font-size: 1em;
        text-transform: uppercase;
        margin-top: 0;
        margin-bottom: 0;

      & p
        margin-top: 0.66rem;
        margin-bottom: 0.66rem;

      & .speaker-link
        display: block;

      & .speaker-photo img
        border: 1px solid white;
// End agenda section


// Start venue section
.venue-address
  background: #32c1d0;
  margin: -2px 0 1em;
  padding: 1rem;

#google-maps-link
  bottom: -15px;
  cursor: pointer;
  position: absolute;
  right: 5px;
  z-index: 10;
// End venue section


// Start sponsors section
#sponsors
  & a.become-sponsor
    margin-bottom: 2.875rem;
    
  & h2
    color: #002447;
    font-size: 1.25em;
    font-weight: 200;
    text-transform: uppercase;
    text-align: center;
    -webkit-text-stroke: .6px rgba(0,36,71,0.8);

  & .partners,
    .media-partners,
    .silver-sponsors,
    .just-sponsors,
    .party-sponsors
    margin-bottom: 1.5rem;
    text-align: center;

    & a
      margin: 0 1em 2em;

.sponsors-slide
  & small
    display: block;
// End sponsors section


// Start bottom part
.organizer
  & p
    margin-top: 1em;

#sml-logo
  margin-bottom: 1em;

.contact-links
  margin-bottom: 10rem;

  & .columns
    padding: 0 .5em 1em;
    text-align: center;

  & img
    margin-right: 0.4em;
    vertical-align: middle;

.team-table
  margin-bottom: 4rem;

  & .columns
    padding: 1rem;

  & img
    border: 1px solid white;

  & h2
    font-size: 1em;
    text-transform: uppercase;
    margin-top: 0;
    margin-bottom: 0;
// End bottom part


// NEWS PAGE
.news-posts.row
  background-image: none;
  padding-top: 1em;
  &:nth-child(odd)
    background-image: none;

  & .news-post
    margin-bottom: 5em;

    & .post-header a
      cursor: default;

    & .social>*
      vertical-align: middle;

    & .fb-like
      margin-left: 10px;
// End news page


::-webkit-input-placeholder
  color: white;
:-moz-placeholder /* Firefox 18- */
  color: white;  
::-moz-placeholder /* Firefox 19+ */
  color: white;
:-ms-input-placeholder 
  color: white;
  

// Start Hotels Page
#hotels-content
  position: absolute;
  z-index: 0;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;

#map
  position:absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 35%;
  z-index:1;
  display: none;
  
  & img
    max-width: inherit;
  
.info-popup
  color: #114476;
  text-shadow: none;
  width: 250px;
  max-height: 124px;
  padding-right: 15px;
  overflow: auto;
  line-height: 1.2;
  
  & h6
    color: #114476;
    font-weight: bold;
    -webkit-text-stroke: 0;
    
    & img
      margin-right: 1px;
      vertical-align: initial;
      &:first-child
        margin-left: 6px;
    
  & p
    font-size: 13px;
    margin-bottom: 1em;
    text-shadow: none;
    
  & a
    color: #2aaac0;
    text-shadow: none;
    &:hover
      color: #2184a4;
      & h6
        color: #2184a4;
        
#hotels section.header
  width: 100%;
  max-width: 100%;
  height: 65px;
  position: absolute;
  z-index:3;
  top:0;
  left: 0;
  right: 0;
  background-color: rgba(125,214,224,0.8);
  box-shadow: -1px 0px 7px rgba(0, 60, 86, 0.3);
    
#hotels-right-section
  background-color: #199fb8;
  overflow-y: auto;
  position: absolute;
  z-index:2;
  left: 0;
  right: 0;
  top:0;
  bottom: 0;
  box-shadow: -1px 0px 7px rgba(0, 60, 86, 0.3);
  padding: 87px 1em 1em;
  
  & a
    color: #9BECF4;
    &:hover
      color: #7cd9e2;
  
  & .hotel-info
    margin-bottom: 2em;
    
    & p
      margin-bottom: 0.5em;
        
    & h6 
      & a
        color: #004c76;
        font-weight: bold;
        margin-right: 3px;
      & img
        margin: 0 1px;
        vertical-align: inherit;

    & b
      color: #004c76;
      font-weight: 600;
      & i
        color: white;
// End Hotels Page


// Start Responsive styles
@media only screen and (min-width: 30em)
  .agenda-table
    & .speaker-photo
      float: left;
      width: 149px;
      margin-right: 1em;

  .team-table img
    float: left;
    width: 149px;
    margin-right: 0.8em;
    
  #hotels-right-section
    padding: 87px 2em 2em;
    

@media only screen and (min-width: 40.063em)
  .top-bar-section ul
    margin-top: 0.7em;

    & li>a
      color: #155380;
      font-size: 1em;
      font-weight: 600;
      transition: color 250ms;

  .top-bar-section li:not(.has-form) a:not(.button)
    background: transparent;
    &:hover
      background: transparent;

  section.row.hero
    padding-top: 6.125em;

    & h1
      font-size: 2.25em;

    & .conference-data
      & time,
      span
        display: inline-block;

  .agenda-table .row
    &.utility .description
      text-align: left;
    & .description
      border-left: 1px  dotted white;

      & .speaker-photo
        width: 125px;

    & .time
      border-bottom: none;

  .team-table img
    width: 125px;

    & [class*="column"]+[class*="column"]:last-child
      float: left;
  
  #map
    display: block;
    
  #hotels-right-section
    left: 65%;
    padding: 87px 1em 2em;
  
  
@media only screen and (min-width: 64.063em)
  .top-bar .name h1 a
    padding: 1em 0 0 1em;

    & img
      max-height: none;

  .top-bar-section ul
    margin-top: 3em;

  .team-table
    & [class*="column"]+[class*="column"]:last-child
      float: left;
      
  #hotels section.header
    height: 109px;
    
  #hotels-right-section
    padding: 130px 2em 2em;
// End Responsive styles
